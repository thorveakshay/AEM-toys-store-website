<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Tab 1">
                <items jcr:primaryType="cq:WidgetCollection">
                    <bannerTitle
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Enter Banner Title"
                        name="./bannerTitle"
                        xtype="textfield"/>
                    <pathOne
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Select Left Image"
                        name="./pathOne"
                        rootPath="/content/dam/summit-toys/thumbnails"
                        xtype="pathfield"/>
                    <pathTwo
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Select Right Image"
                        name="./pathTwo"
                        rootPath="/content/dam/summit-toys/thumbnails"
                        xtype="pathfield"/>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
